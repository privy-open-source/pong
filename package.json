{
  "name": "@privyid/pong",
  "version": "0.2.1",
  "packageManager": "yarn@4.0.2",
  "description": "Service discovery, Log & Trace module",
  "repository": {
    "type": "git",
    "url": "https://github.com/privy-open-source/pong.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./core/*": "./dist/core/*"
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "yarn build",
    "build": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint . --ext .js,.vue,.ts --format pretty",
    "fix": "yarn lint --fix",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^4.0.0",
    "@nuxt/kit": "^3.6.5",
    "browserslist-ua-parser": "^1.0.1",
    "dd-trace": "^4.11.0",
    "defu": "^6.1.2",
    "destr": "^2.0.1",
    "nanoid": "^5.0.0",
    "pino-http": "^8.3.3"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/module-builder": "^0.5.0",
    "@nuxt/schema": "^3.6.5",
    "@nuxt/test-utils": "^3.6.5",
    "@privyid/eslint-config-persona": "^0.24.0",
    "@privyid/nhp": "^0.4.0",
    "@privyid/nuapi": "^0.2.1",
    "@types/node": "^20.0.0",
    "@types/simple-oauth2": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vue/eslint-config-typescript": "^12.0.0",
    "changelogen": "^0.5.4",
    "eslint": "^8.35.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-plugin-align-assignments": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-varspacing": "^1.2.2",
    "eslint-plugin-vue": "^9.9.0",
    "nuxt": "^3.6.5",
    "typescript": "^5.1.6",
    "vitest": "^0.34.0"
  },
  "peerDependencies": {
    "@privyid/nuapi": "*"
  },
  "peerDependenciesMeta": {
    "@privyid/nuapi": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
